// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract NTST1155 is ERC1155, AccessControl {
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    //Name of token
    string public name;

    // Mapping for token URIs
    //tokenURIs[tokenId] = tokeURI
    mapping(uint256 => string) private tokenURIs;

    constructor(
        string memory _name, 
        string memory _baseUri,
        address minter
        ) ERC1155(_baseUri) {
        name = _name;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender)  ;
        _setRoleAdmin(MINTER_ROLE, ADMIN_ROLE);
        _grantRole(ADMIN_ROLE, minter);
        _grantRole(MINTER_ROLE, minter);
    }

    /**
     * @dev Sets `tokenURI` as the tokenURI of `tokenId`.
     */
    function setURI(uint256 tokenId, string calldata tokenURI)
        external
        virtual
        onlyRole(MINTER_ROLE)
    {
        tokenURIs[tokenId] = tokenURI;

        emit URI(tokenURI, tokenId);
    }

    /**
     * @notice Create new token
     * @param _to Target address
     * @param _id Id of token type
     * @param _amount Amount of tokens for mint
     * @param _data Additional data with no specified format
     */
    function mint(
        address _to,
        uint256 _id,
        uint256 _amount,
        bytes memory _data
    ) public onlyRole(MINTER_ROLE) {
        _mint(_to, _id, _amount, _data);
    }

    /**
     * @notice Create new tokens
     * @param _to Target address
     * @param _ids IDs of each token type (order and length must match _values array)
     * @param _amounts Amounts per token type for mint
     * @param _data Additional data with no specified format
     */
    function mintBatch(
        address _to,
        uint256[] memory _ids,
        uint256[] memory _amounts,
        bytes memory _data
    ) public onlyRole(MINTER_ROLE) {
        _mintBatch(_to, _ids, _amounts, _data);
    }

    /**
     * @notice Burn token
     * @param _from Target address
     * @param _id Id of token type
     * @param _amount Amount of tokens for mint
     */
    function burn(
        address _from,
        uint256 _id,
        uint256 _amount
    ) public onlyRole(MINTER_ROLE) {
        _burn(_from, _id, _amount);
    }

    /**
     * @notice Burn batch of tokens
     * @param _from Target address
     * @param _ids IDs of each token type (order and length must match _values array)
     * @param _amounts Amounts per token type for burn
     */
    function burnBatch(
        address _from,
        uint256[] memory _ids,
        uint256[] memory _amounts
    ) public onlyRole(MINTER_ROLE) {
        _burnBatch(_from, _ids, _amounts);
    }

    /**
     * @dev Returns `tokenURI` using `tokenId`.
     */
    function uri(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        string memory tokenURI = tokenURIs[tokenId];

        return bytes(tokenURI).length > 0 ? tokenURI : super.uri(tokenId);
    }
    
    //Lock transfer of tokens
    function _safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) internal virtual override {
        require(false, "Token is non-transferable");
    }

    //Lock transfer of tokens
    function _safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal virtual override {
        require(false, "Token is non-transferable");
    }

    /**
     * @notice The following functions are overrides required by Solidity.
     * @dev See {IERC165-supportsInterface}.
     * @param _interfaceId interface id
     */
    function supportsInterface(bytes4 _interfaceId)
        public
        view
        override(ERC1155, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(_interfaceId);
    }
}
